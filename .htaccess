<IfModule mod_rewrite.c>
    RewriteEngine On

    # Don't rewrite requests to real files or directories
    RewriteCond %{REQUEST_FILENAME} -f [OR]
    RewriteCond %{REQUEST_FILENAME} -d
    RewriteRule ^ - [L]

    # Send all other requests to index.php
    RewriteRule ^ index.php [L]
</IfModule>
